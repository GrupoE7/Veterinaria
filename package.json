{
  "name": "vue-material-kit",
  "author": "Creative Tim https://www.creative-tim.com/",
  "description": "Vue Material Kit",
  "version": "1.2.2",
  "engines":{
    "node": "^16.10.0"
  },
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build && gulp licenses",
