    "sweetalert": "^2.1.2",
    "v-tooltip": "2.0.2",
    "vue": "2.6.10",
    "vue-axios": "^3.3.7",
    "vue-carousel": "0.18.0",
    "vue-clickaway": "2.2.2",
    "vue-lazyload": "1.3.3",
    "vue-material": "1.0.0-beta-11",
    "vue-router": "3.1.3"
